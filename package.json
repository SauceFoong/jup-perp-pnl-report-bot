{
  "name": "jup-perp-pnl-reporter",
  "version": "0.0.1",
  "description": "A sample project to work with the Jupiter Perps IDL with Anchor",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/poll-my-current-pnl.js",
    "dev": "ts-node-dev --respawn --transpile-only src/poll-my-current-pnl.ts",
    "script": "ts-node-dev --respawn --transpile-only --",
    "poll-pnl": "ts-node-dev --respawn --transpile-only src/examples/poll-my-current-pnl.ts"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.95.3",
    "@types/dotenv": "^6.1.1",
    "@types/node-telegram-bot-api": "^0.64.11",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
